    //int a=SC.nextInt();
    int a=653505;
    int digit=0;
    if(a<0){
      System.out.println("The number is negative,please enter positive interger");}
    else if(a==0){
      System.out.println("Then number is 0");}
    else{
      while(a>0){
        a=a/10;
        digit ++;}
      System.out.println(digit);}}
