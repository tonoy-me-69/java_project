public class matrixAddition{
  
  public static void main(String [] args){
    int[][] A = {{1,2}, {3,4}};
    int[][] B = {{5,6}, {7,8}};
    int[][] C = new int [A.length][A[0].length];
    
    for(int i=0; i<A.length ; i++ ){
      for(int j=0; j<A[0].length ; j++){
        C[i][j]+= A[i][j]+ B[i][j];
      }
    }
    for(int i=0; i<A.length ; i++ ){
      for(int j=0; j<A[0].length ; j++){
        System.out.println(C[i][j]);
      }
    }
  }
}
    
    
    
    
    